<% errors = model.errors %>

<% if errors.any? %>
  <div class="col-12 alert alert-danger alert-dismissible">
    <div class="lead buffer-bottom">
      <%= t('errors.messages.not_found', count: errors.count) %>
    </div>
    <ul>
      <% errors.each do |message| %>
        <li><%= message.full_message %></li>
      <% end %>
    </ul>
  </div>
<% end %>
